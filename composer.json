{
    "name": "rafayrty/linkedlist-php",
    "description": "This Package allows you to have a Linked List inside Your PHP Project",
    "keywords": [
        "rafayrty",
        "linkedlist-php"
    ],
    "homepage": "https://github.com/rafayrty/linkedlist-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Rafay",
            "email": "farhatiqbal@protonmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Rafayrty\\LinkedlistPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rafayrty\\LinkedlistPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
